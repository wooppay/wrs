<div class="tab-pane" id="nav-report" role="tabpanel" aria-labelledby="nav-report-tab">
    <div class="row">
        <div class="col-sm-8">
            <div class="card">
                <div class="card-body">
                    {{ form_start(form, {'attr': {'id':'user_report_form'} }) }}
                    <span class="invalid-feedback d-block">
                        <span class="d-block">
                            <span id="user" class="form-error-message"></span>
                        </span>
                    </span>

                    {{ form_row(form.user) }}
                    {{ form_row(form.dateFrom) }}
                    {{ form_row(form.dateTo) }}

                    {{ form_end(form, {}) }}
                    <button class="btn btn-primary" id="get-report-btn"
                            onclick="displayReport()">Предварительный просмотр
                    </button>
                </div>
            </div>

        </div>
    </div>
</div>

